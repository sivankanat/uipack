    "autoprefixer": "^10.2.5",
    "browser-sync": "^2.26.14",
    "browserlist": "^1.0.1",
    "cssnano": "^5.0.4",
    "ejs": "^3.1.6",
    "node-sass": "^5.0.0",
    "postcss": "^8.3.0",
