- var pageTitle = 'Separator'
doctype html
html(lang="en")
  head
    include ../includes/head.pug
  body
    div.container
      h1 UIPack:#{pageTitle}
      include ../includes/nav.pug

      h3 default
      hr

      h3(class='mt:md') circle
      code hr(class='separator' separator='circle')
      hr(class='separator' separator='has:circle')
